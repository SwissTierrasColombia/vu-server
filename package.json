        "babel-preset-stage-2": "^6.24.1",
        "babel-register": "^6.26.0",
        "concurrently": "^3.5.1",
        "eslint": "^4.19.1",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.1",
        "gulp-json-editor": "^2.3.0",
        "gulp-minifier": "^1.2.2",
        "gulp-rename": "^1.2.2",
        "gulp-rimraf": "^0.2.2",
        "mocha": "^5.1.1",
        "morgan": "^1.9.0",
        "nodemon": "^1.17.3",
